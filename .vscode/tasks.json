{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}/src/SessionizeCfpViewer/SessionizeCfpViewer.csproj",
				"/property:GenerateFullPaths=true",
				"/consoleloggerparameters:NoSummary;ForceNoAlign"
			],
			"problemMatcher": "$msCompile",
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "publish",
			"command": "dotnet",
			"type": "process",
			"args": [
				"publish",
				"${workspaceFolder}/src/SessionizeCfpViewer/SessionizeCfpViewer.csproj",
				"/property:GenerateFullPaths=true",
				"/consoleloggerparameters:NoSummary;ForceNoAlign"
			],
			"problemMatcher": "$msCompile"
		},
		{
			"label": "watch",
			"command": "dotnet",
			"type": "process",
			"args": [
				"watch",
				"run",
				"--project",
				"${workspaceFolder}/src/SessionizeCfpViewer/SessionizeCfpViewer.csproj"
			],
			"problemMatcher": "$msCompile",
			"isBackground": true
		},
		{
			"label": "Run Blazor App",
			"type": "shell",
			"command": "dotnet run",
			"options": {
				"cwd": "${workspaceFolder}/src/SessionizeCfpViewer"
			},
			"isBackground": false,
			"problemMatcher": []
		}
	]
}